import java.util.*;
public class Averageweight{
 public static void main(String[] arg)
 {
   int sum=0,Average;
   float weight;
   Scanner sc=new Scanner(System.in);
   for(int i=1;i<=10;i++)
   {
     System.out.println("enter the weight of people"+i);
     weight=sc.nextFloat();
     sum+=weight; 
   }
   Average=sum/10;
   System.out.println(Average); 
 }
}
